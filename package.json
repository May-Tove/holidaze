{
  "name": "holidaze",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run",
    "format": "prettier --w \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint-fix": "eslint \"src/**/*.{js,jsx}\" --cache --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-modules-commonjs": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "cypress": "^12.12.0",
    "eslint": "^8.38.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-axios": "^4.7.2",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "vite": "^4.2.0"
  },
  "jest": {
    "setupFiles": [
      "jest-mock-axios"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(module\\.css|module\\.less)$": "identity-obj-proxy",
      "\\.(css|less|jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
